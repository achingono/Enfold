<?xml version="1.0" encoding="utf-8"?>
<package xmlns="http://schemas.microsoft.com/packaging/2012/06/nuspec.xsd">
    <metadata>
        <id>Enfold.Javascript</id>
        <version>$version$</version>
        <title>Enfold.Javascript</title>
        <authors>Alfero Chingono</authors>
        <owners>Alfero Chingono</owners>
        <projectUrl>https://github.com/achingono/Enfold</projectUrl>
        <requireLicenseAcceptance>false</requireLicenseAcceptance>
        <description>Convention based generation of dynamic bundles</description>
        <copyright>Copyright Â© Alfero Chingono</copyright>
        <dependencies>
            <group targetFramework=".NETFramework4.5">
                <dependency id="Microsoft.AspNet.Web.Optimization" />
            </group>
        </dependencies>
        <frameworkAssemblies>
            <frameworkAssembly assemblyName="Microsoft.CSharp" targetFramework=".NETFramework4.5" />
            <frameworkAssembly assemblyName="Microsoft.Web.Infrastructure" targetFramework=".NETFramework4.5" />
